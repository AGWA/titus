#!/bin/sh

set -e

if [ "$1" = "configure" ]; then

	# Create users
	getent passwd titus >/dev/null || \
		adduser --quiet --system --home /nonexistent --no-create-home titus

	getent passwd titus-keys >/dev/null || \
		adduser --quiet --system --home /nonexistent --no-create-home titus-keys
fi

#DEBHELPER#

exit 0;
